{
  "name": "Age Checker",
  "version": "1.0.0",
  "description": "App to give your exact age",
  "manifest_version": 3,
  "author": "Rathish Sekar",
  "action":{
      "default_popup": "index.html",
      "default_title": "Your Age",
      "default_icon": {
        "16": "/images/a16.png",
        "24": "/images/a24.png",
        "32": "/images/a32.png",
        "64": "/images/a64.png",
        "128": "/images/a128.png"
      }
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage"],
  "icons": {
    "16": "/images/a16.png",
    "24": "/images/a24.png",
    "32": "/images/a32.png",
    "64": "/images/a64.png",
    "128": "/images/a128.png"
  }
}